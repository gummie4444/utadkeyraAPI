    "dotenv": "^5.0.1",
    "express": "^5.0.0-alpha.2",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-iso-date": "^3.5.0",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql2": "1.5.3",
    "sequelize": "4.37.6",
    "sequelize-cli": "4.0.0"
  }
